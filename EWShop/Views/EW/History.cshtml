            padding: 4px;
            line-height: 1.4;
        }
        table.dataTable thead th {
            padding: 2px;
            white-space: nowrap;
            text-align: center;
            font-weight: 500;
            color: black;
        }

        table.dataTable thead tr:first-child th, table.dataTable tfoot th {
            padding: 5px;
            text-align: center;
            font-weight: 600;
        }

        table.dataTable tbody td {
            padding: 4px 7px;
            white-space: nowrap;
            vertical-align: middle;
            font-size: 14px;
            overflow: hidden;
            text-overflow: ellipsis;
        }

        table.dataTable tbody tr.active td {
            font-weight: 600;
            background-color: #ddd;
        }

        table.dataTable tbody tr {
            cursor: pointer;
        }

        .div-dialog {
            display: none;
        }

        .form-control {
            height: 26px;
            padding: 5px;
        }
        .btn-view {
            padding: 2px 15px !important;
        }

        .form-filter {
            width: 100% !important;
        }
        td.details-control {
            background: url('../Libraries/dist/img/details_open.png') no-repeat center center;
            cursor: pointer;
        }
        tr.shown td.details-control {
            background: url('../Libraries/dist/img/details_close.png') no-repeat center center;
        }
        .select2-container--default .select2-selection--single .select2-selection__arrow, .select2-container .select2-selection--single, .select2-container--default .select2-selection--single .select2-selection__rendered{
            height: 26px!important;
        }
        .row-no-margin{
            margin:0;
        }
        button.dt-button{
            padding: 2px 10px;
            font-size: 14px;
            margin:0;
            margin-bottom: 5px;
        }
        .div-table{
            margin: 0!important;
            display:none;
            padding: 0 15px;
        }
        .js--image-preview{
            height: 150px;
        }
        .js--image-preview::after {
            top: calc(50% - 1rem);
            left: calc(50% - 2.5rem);
        }
        .upload-options{
            height: 35px;
        }
        .upload-options label::after{
            font-size: 25px;
        }
        .box{
            height: 187px;
            margin: 2px;
            min-width: 10px;
        }
        .col-md-4{
            padding: 0 5px;
        }
        .dataTables_wrapper .dataTables_paginate{
            padding-bottom:0;
        }
        .panel{
            margin-bottom: 5px;
        }
        .select2-results__option{
            padding: 4px;
        }
        .btn {
            padding: 3px 15px;
            font-size: 14px;
        }
        .ui-autocomplete {
            max-height: 150px;
            overflow-y: auto; /* prevent horizontal scrollbar */
            overflow-x: hidden; /* add padding to account for vertical scrollbar */
            z-index: 1000 !important;
        }
    </style>
}
